@model IEnumerable<E_Commerce_Website.Models.Order>

<h2>Order List</h2>
<table class="table">
    <thead>
        <tr>
            <th>Order ID</th>
            <th>Product ID</th>
            <th>Customer ID</th>
            <th>Quantity</th>
            <th>Total Price</th>
            <th>Order Date</th>
            <th>Status</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.order_id</td>
                <td>@order.prod_id</td>
                <td>@order.order_id</td>
                <td>@order.quantity</td>
                <td>@order.total_price.ToString("C")</td>
                <td>@order.order_date.ToShortDateString()</td>
                <td>@order.order_status</td>
            </tr>
        }
    </tbody>
</table>
